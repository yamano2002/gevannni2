{
  "name": "gevanni2",
  "version": "1.0.0",
  "description": "Group List Management System",
  "author": "",
  "private": true,
  "scripts": {
    "backend-dev": "pm2-dev start config/pm2.yml --auto-exit",
    "backend-production": "pm2-runtime start config/pm2.yml",
    "user-dev": "nuxt -c config/nuxt.user.js",
    "user-build": "nuxt build -c config/nuxt.user.js",
    "user-build:staging": "env NUXT_ENV_GVN_ENV=\"staging\" yarn user-build",
    "user-build:production": "env NUXT_ENV_GVN_ENV=\"production\" yarn user-build",
    "admin-dev": "nuxt -c config/nuxt.admin.js",
    "admin-build": "nuxt build -c config/nuxt.admin.js",
    "admin-build:staging": "env NUXT_ENV_GVN_ENV=\"staging\" yarn admin-build",
    "admin-build:production": "env NUXT_ENV_GVN_ENV=\"production\" yarn admin-build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "env NODE_ENV=test jest -i --forceExit",
    "precommit": "yarn lint",
    "change-admin-password": "node ./commands/change-admin-pswd"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "globalSetup": "<rootDir>/tests/globalSetup.js",
    "globalTeardown": "<rootDir>/tests/globalTeardown.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/moment": "^1.1.0",
    "@nuxtjs/style-resources": "^0.1.1",
    "@nuxtjs/vuetify": "^0.5.1",
    "bcrypt": "^3.0.3",
    "case": "^1.6.0",
    "csv-parse": "^4.4.1",
    "fastify": "^1.13.3",
    "fastify-plugin": "^1.4.0",
    "file-saver": "^2.0.1",
    "ioredis": "^4.3.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mysql2": "^1.6.4",
    "nanoid": "^2.0.0",
    "nodemailer": "^5.1.1",
    "nodemailer-mailgun-transport": "^1.4.0",
    "nuxt": "^2.0.0",
    "nuxt-client-init-module": "^0.1.4",
    "papaparse": "^4.6.3",
    "sequelize": "^4.42.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "v-runtime-template": "^1.5.2",
    "verte": "^0.0.10"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^24.0.0",
    "maildev": "^1.0.0-rc3",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3"
  }
}
